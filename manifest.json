{
    "manifest_version": 3,
    "name": "Copy Markdown Link",
    "version": "1.0",
    "description": "Copies the current tab's URL and title as a Markdown link",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "clipboardWrite"
    ],
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
